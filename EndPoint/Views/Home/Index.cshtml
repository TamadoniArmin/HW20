@{
    ViewData["Title"] = "Home Page";
}
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>
@if (TempData["systom Error"] != null)
{
    <div class="alert alert-danger">@TempData["systom Error"] </div>
}
@if (TempData["succes in setting Request"] != null)
{
    <div class="alert alert-success">@TempData["succes in setting Request"] </div>
}
